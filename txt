       championBackground = myLibrary[i].name;
        switch(championBackground){
            case "Aurelion Sol":
                championBackground = "AurelionSol";
                break;
            case "Cho gath":
                championBackground = "Chogath";
                break;
            case "Dr Mundo":
                championBackground = "DrMundo";
                break;
            case "Jarvan IV":
                championBackground = "JarvanIV";
                break;
            case "Kog Maw":
                championBackground = "Kog Maw";
                break;
            case "Master Yi":
                championBackground = "MasterYi";
                break;
            case "Miss Fortune":
                championBackground = "MissFortune";
                break;
            case "Wukong":
                championBackground = "MonkeyKing";
                break;
            case "Tahm Kench":
                championBackground = "TahmKench";
                break;
            case "Twisted Fate":
                championBackground = "TwistedFate";
            case "Xin Zhao":
                championBackground = "XinZhao";
                break;
            default:
                break;
        }

        championCard.style.backgroundImage = `url(assets/splash/${championBackground}_0.jpg)`;